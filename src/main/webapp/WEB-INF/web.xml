	<servlet-mapping>
	    <servlet-name>dispatcher</servlet-name>
	    <url-pattern>*.com</url-pattern>
	</servlet-mapping>
  
  
	<session-config>
		<session-timeout>5</session-timeout>
	</session-config>
  
